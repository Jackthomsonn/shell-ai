{
  "name": "shell-ai",
  "version": "1.0.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev:start --parallel",
    "lint": "turbo run lint",
